!EditTemplate
<!--{{{-->
<div class="portletContainer">
	<div class="portletToolbar">
		<div class="portletCornerLeft"></div>
		<div class="portletCornerRight"></div>
		<div class="portletTitle">
			<div class='title' macro='view title'></div>
		</div>
		<div class='toolbar' macro='toolbar [[ToolbarCommands::EditToolbar]]'></div>
	</div>
	<div class="portletContent">
		<div class='editor' macro='edit title'></div>
		<div macro='annotations'></div>
		<div class='editor' macro='edit text'></div>
		<div class='editor' macro='edit tags'></div><div class='editorFooter'><span macro='message views.editor.tagPrompt'></span><span macro='tagChooser'></span></div>
	</div>
	<div class="portletFooter">
		<div class="portletBotLeft"></div>
		<div class="portletBotRight"></div>
	</div>
</div>
<!--}}}-->

!ViewTemplate
<!--{{{-->
<div class="portletContainer">
	<div class="portletToolbar">
		<div class="portletCornerLeft"></div>
		<div class="portletCornerRight"></div>
		<div class="portletTitle">
			<div class='title' macro='view title'></div>
		</div>



		<div class='toolbar' macro='toolbar [[ToolbarCommands::ViewToolbar]]'></a>
</div>
	</div>
	<div class="portletContent">
		<div class='viewer' macro='view text wikified'></div>
		<div class='subtitle'><br /> Last edited by: <span macro='view modifier link'></span>, <span macro='view modified date'></span> (<span macro='message views.wikified.createdPrompt'></span> <span macro='view created date'></span>)</div>
		<div class='tagging' macro='tagging'></div>
		<div class='tagged' macro='tags'></div>
		<div class='tagClear'></div>
	</div>
	<div class="portletFooter">
		<div class="portletBotLeft"></div>
		<div class="portletBotRight"></div>
	</div>
</div>
<!--}}}-->


!wizardViewTemplate
<!--{{{-->
<div class="portletContainer">
	<div class="portletToolbar">
		<div class="portletCornerLeft"></div>
		<div class="portletCornerRight"></div>
		<div class="portletTitle">
			<div class='title' macro='view title'></div>
		</div>
	<div class='toolbar' macro='toolbar closeTiddler closeOthers'></div>
	</div>
	<div class="portletContent">
		<div class='viewer' macro='view text wikified'></div>
	</div>
	<div class="portletFooter">
		<div class="portletBotLeft"></div>
		<div class="portletBotRight"></div>
	</div>
</div>
<!--}}}-->


!tabs
<<tabs txtMainTab "Timeline" "Timeline" TabTimeline "All" "All tiddlers" TabAll "Tags" "All tags" TabTags "More" "More lists" TabMore>>

!OptionsPanel
[[help|Help]] <br />[[settings|AdvancedOptions]]<br /><<ccOptions>>

!SideBarOptions
<<search "search...">><<newTiddler label:"new note" text:"New Note" title:"New Note" tag:"note">><<closeAll>><<saveChanges>><<slider chkSliderOptionsPanel mybtTheme##OptionsPanel 'options »' 'change TiddlyWiki Options'>><<slider chkSliderTabs mybtTheme##tabs 'contents »' 'View TiddlyWiki Tabs'>>


!StyleSheet
/***
General
***/
/*{{{*/


body {
background-color:#E8E1F1;
}

div.title {
	font-size:1em;
	color:black;
}

div.portletContent {
	padding:0.8em;

}

#sidebarOptions .searchButton {
	display:none;
}


.button {
	margin-top:1em;
	border:0px;
}

.button:hover {
	background-color:transparent;
	font-weight:bold;
}

div.columnThree {
	float:right;
	width:20%;
	margin-top:1em;
	margin-right:2em;
}

div.columnOne {
	margin-top:1em;
	margin-left:2em;
	width:16%;
	position:fixed;
	float:left;
}



div.wizard, div.wizardFooter, div.wizardStep {
	border:white;
	background-color:white;
}

.wizard {
background-color:white;
border:0px;
}


#sidebarOptions .sliderPanel {
	background:white none repeat scroll 0 0;
}

a.tiddlyLink, a.tab, a.externalLink {
	color:#871497 !important;
}

a.tabUnselected, a.tabUnselected:hover {
	border:1px;
	background-color:#ddd !important;
}

a.tabSelected,a.tabSelected:hover  {
	border:1px;
	padding-bottom:1em;
	background-color:none !important;
}

.wizard .button{
	background-color:#E8E1F1;
	border:1px solid #444;
	color:black;
	padding:0.5em;
}

.wizard .button:hover{
	font-weight:bold;
}

div.annotation {
	background-color:#E8E1F1;
	border:1px solid black;
}

#messageArea {
	background-color:white;
	border:1px solid black;
}
						
/*}}}*/
</pre>
</div>
