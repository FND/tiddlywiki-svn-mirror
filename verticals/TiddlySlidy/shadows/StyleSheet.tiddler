/*{{{*/
.themeButton {
    width: 5em;
    font-weight: bold;
    text-align: center;
    background-color: #EEE;
    border: 2px solid #EEE;
    color: #333;
}

.themeButton.selected {
    background-color: #07E;
    border: 2px solid #07E;
    color: #FFF;
    border-radius: 4px;
    -moz-border-radius: 4px;
    -webkit-border-radius: 4px;
}
/*}}}*/
