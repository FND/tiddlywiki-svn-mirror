/* MyPage Specifics */

	html body .body div.header {
		background:#fff none repeat scroll 0 0 !important;
	}

	html body .body .sections {
		background: #F3F3F3;
	}

	#tab_wrapper ul li {
		border:medium none; 
		list-style-image:none;
		list-style-position:outside;
		list-style-type:none;
		margin:0;
		padding:0;
	}

	html body #search_bar {
		background:#FFFFFF none repeat scroll 0 0;
		display:block;
		height:auto;
	}
	html body #tab_wrapper {
		background: transparent url(/doccollab/static/mydocs_images/tabs_bg.png) repeat-x scroll left top;
	}
	
	html body #header2 {
		position:relative;
	}
	
	
	#tab_bar {
		visibility: visible;
	}
	
/* END MyPage Specifics */


.links {
