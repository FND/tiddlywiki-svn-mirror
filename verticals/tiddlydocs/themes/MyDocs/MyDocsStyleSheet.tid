/* TIDDLYWIKI CSS */

	.editor textarea {
		background-color:#fff;
		width:98.5%;
	}

	html body #mainMenu {
		width:100%;
		position:relative;
		left:-1em;
		top:-1em;
	}


	html body #tiddlerDisplay {
		float:right;
		height:100%;
		margin:0 2em 0 0;
		overflow:hidden;
		padding:0;
		top:-1.5em;
		position:relative;
		width:66%;
	}

	div.tiddler {
		background-color:transparent;
		border:0px;
		margin-right:1em;
		padding:0em;
	}

/*WIZARD*/

	form.wizard, .wizard{
		background:#F3F3F3 none repeat scroll 0 0 !important;
		border:1px solid #eee;
		color:#555;
	}

	 .wizardFooter {
		border:0px;
		background:transparent;
	}

	div.wizardBody div.wizardStep {
		border:1px solid #eee;
		background:##F3F3F3;
		padding:1em;
	}

	.wizard .button {
		background:#5586B6 none repeat scroll 0 0 !important;
		color:white !important;
		padding:0.2em;
		padding-left:0.4em !important;
		padding-right:0.4em !important;
		font-weight:bold;
	}

	div.wizardFooter {
		margin-left :9.2em;
	}

/*ENDWIZARD*/

	.clear { /* generic container (i.e. div) for floating buttons */
	    overflow: hidden;
	    width: 100%;
	}

	html body .viewer {
		padding:1.5em 1.5em 0;
		top:top:-1.8em; 
		z-index:1;	
		position:relative;
		top:-1.2em;
	}
	.wizardViewer {
		padding:1em;
	}

	.viewer th, .viewer thead td, .twtable th, .twtable thead td  {
		 background:#5586B6 none repeat scroll 0 0;
	}

	.viewer th, .viewer thead td, .twtable th, .twtable thead td {
		border:1px solid #5586B6;
	}

	.toolbar {
		font-size:0.6em;		
	}

	.toolbar  > a {
		top:1px;
		position:relative;
	}

	.commentStretch {
		border:0px;
		border-top: 1px solid #C4C4C4;
		background:#eee;
		width:100%;	
		min-height:1.9em;
	}
	
	div.commentStretch  a.btn  span, div.commentStretch  a.btn  span span { 
	background:transparent;
	border:0px;
	top:0.3em;
		}
	
	

/* END TIDDLYWIKI CS */

/*  TEAK TASKS CSS */

	.viewer table, table.twtable, .viewer td, .viewer tr, .twtable td, .twtable tr {
		border:0px !important;
	}
	
	.taskControls, .TaskTiddlerControls {
		z-index:3;
	}

/*  END TEAM TASKS CSS */

/*  COMMENTS CSS */

	html body .comments textarea {
		width:98%;	
	}
	.commentsArea {
		padding:2em;
		text-align:center;
		background:#fff;
	}
	html body .comment .replyLink {
		color:black;
	}
	html body .commentText {
		color:#555;
	}
	 .comment .heading {
		background:#F3F3F3 none repeat scroll 0 0 !important;
		padding:0.5em;
		background:transparent;
		border-bottom:1px solid #F3F3F3 !important;
		border-right : 0px none #fff !important;
	}
	
	.replyLink {
		padding:1em;
	}

	.comment .reply {
		width:95%;
	}
	
	.commentTitle {
		color:#666;
	}



	.comment {
		border:1px solid #F3F3F3;
		padding:1em;
	}

	.comments {
		padding:0.5em;
		text-align:left;
	}

	.comments .newCommentArea {
		text-align:center;
		padding-top:2em;
	}

	.commentText {
		padding:1em !important;
		background-color:transparent;
		color:#eee
	}

	.tiddler .button {
		background-color:white;
		border:1px solid #DDDDDD;
		margin:6.1em 0.2em;
	}

/*  END COMMENTS CSS */

