.welcome h3{font-size:1.3em;font-weight:bold;margin:3px 0;padding:0;text-transform:uppercase;}
.welcome a:visited:hover{font-weight:bold;line-height:1.35em;text-decoration:underline;}
.welcome a:hover{color:#039;font-weight:bold;line-height:1.35em;text-decoration:underline;}
.welcome a:visited{color:#69C!important;font-weight:bold;line-height:1.35em;text-decoration:none;}
