.selectedtab{background-color:#FFF;font-weight:bold;color:#606060;}
.unselectedtab{color:#FFF;cursor:pointer;background-color:#555;border:1px solid #FFF;height:40px;}
.tab span{display:inline;padding-left:10px;padding-right:5px;white-space:pre;}
.email{font-weight:bold;}
#ajax_message{background-color:#888;color:#FFF;display:inline;font-weight:bold;opacity:.5;padding:3px 10px 6px 10px;vertical-align:middle;}
#ajax_message_wrapper{display:table-cell;height:100%;text-align:center;vertical-align:middle;width:100%;visibility:hidden;}
.gadgets-gadget-top-dummy,.gadgets-gadget-bottom-dummy{width:100%;}
.gadgets-gadget{border:0;padding:0;padding-left:0;margin:0;width:100%;}
