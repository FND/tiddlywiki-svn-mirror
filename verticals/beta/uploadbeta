#!/bin/bash
#  uploadbeta release
DEFAULT_RELEASE=2.4.0.B2
RELEASE=${1:-$DEFAULT_RELEASE}
scp index.$RELEASE.html http://tiddlywiki.com:./temp/index.html
scp empty.$RELEASE.html http://tiddlywiki.com:./temp/empty.html
ssh http://tiddlywiki.com "sudo mv ./temp/index.html XXX/beta/index.html"
ssh http://tiddlywiki.com "sudo cp ./temp/empty.html XXX/beta/empty.html"
ssh http://tiddlywiki.com "sudo mv ./temp/empty.html XXX/upgrade/index.html"
